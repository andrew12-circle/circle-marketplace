# Supabase Configuration
VITE_SUPABASE_PROJECT_ID="your-project-id"
VITE_SUPABASE_PUBLISHABLE_KEY="your-anon-key"
VITE_SUPABASE_URL="https://your-project.supabase.co"

# Site Configuration
NEXT_PUBLIC_SITE_URL="http://localhost:3000"

# Turnstile Configuration (for signup protection)
REACT_APP_TURNSTILE_SITE_KEY="your-turnstile-site-key"
TURNSTILE_SECRET_KEY="your-turnstile-secret-key"

# Google OAuth (optional)
NEXT_PUBLIC_GOOGLE_OAUTH_ON="false"

# PostHog Analytics Configuration (optional)
# Set to "true" to completely disable PostHog
VITE_POSTHOG_DISABLE="false"
# Set to "true" for strict throttling (recommended for development)
VITE_POSTHOG_STRICT="false"
# Maximum events per second (default: 5)
VITE_POSTHOG_MAX_PER_SECOND="5"
# Maximum events per minute (default: 50)
VITE_POSTHOG_MAX_PER_MINUTE="50"
# Debounce time for similar events in ms (default: 500)
VITE_POSTHOG_DEBOUNCE_MS="500"
# Set to "false" to disable admin route throttling
VITE_POSTHOG_ADMIN_THROTTLE="true"